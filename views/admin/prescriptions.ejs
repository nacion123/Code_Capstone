<%- include('../partials/admin/contentheader') %>
    <!-- Main Body (content)-->
    <main class="main users chart-page" id="skip-target">
      <div class="container">

        <br>

        <div>
          <div class="col col-xs-6">
            <h3 style="color: #1e140a;"><b>HISTORY:</b></h3>
            <br>
        </div>
          <br>

          
          <% if(error && error.length > 0){ %>
            <div class="alert alert-danger" role="alert">
                <%= error %>
            </div>
        <% } %>
        <% if(success && success.length > 0){ %>
            <div class="alert alert-success" role="alert">
                <%= success %>
            </div>
        <% } %>
        <% if(info && info.length > 0){ %>
            <div class="alert alert-info" role="alert">
                <%= info %>
            </div>
        <% } %>

          <!-- Table -->
          <table class="table table-striped table-bordered table-list">
            <thead>
              <tr>
                  <th><center><em class="fa fa-cog"></em></center></th>
                  <th><center>Room Name</center></th>
                  <th><center>Room Type</center></th>
                  <th><center>Room Floor</center></th>
                  <th><center>Room Person</center></th>
              </tr>
            </thead>
  
            <tbody>
                  <% rows2.forEach((row, i) => { %>
                    <tr>
                      <td align="center">
                        <button type="button" class="btn btn-default">
                          <a href="remove/<%=row.ROOM_ID%>" class="btn btn-danger" style="background-color:#800000; border-color: transparent;">Delete</a>
                        </button>
                      </td>
                      <td><center> <%=row.ROOM_NUM%> </center></td>
                      <td><center> <%=row.FLOOR%> </center></td>
                      <td><center> <%=row.ROOM_TYPE%> </center></td>
                      <td><center> <%=row.ROOM_PERSON%> </center></td>

                    </tr>
                  <% }) %>
            </tbody>
          </table>
        
        </div>
      </div>
    </main>

<%- include('../partials/footer') %>